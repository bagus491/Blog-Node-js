<div class="container">
  <br>
  <div class="content-blog" style="display: flex; flex-wrap: wrap;">
      <%  getPost.forEach(e => { %>
        <div class="card" style="width: 18rem; margin-left: 10px;">
          <img src="<%= e.Avatar %>" class="card-img-top img-flui" alt="<%= e.Slug %>" style="height: 300px;" >
          <div class="card-body">
            <h5 class="card-title"><%= e.Title %></h5>
            <p class="card-text"><%= e.Preparagraf %></p>
            <a href="/readblog/<%= e.Slug %>" class="btn btn-primary">Baca Artikel</a>
          </div>
        </div>

      <% }) %>

  </div>

  <nav aria-label="Page navigation example" style="margin-left: 10px; margin-top: 50px;">
    <ul class="pagination">
      <% for( let i = 1; i <= jmlhHalaman; i++ ) { %>
        <% if (lock) { %>
          <li class="page-item"><a class="page-link" href="/?search_query=<%= search %>&page=<%= i %>"><%= i %></a></li>
        <% } else {%>
        <li class="page-item"><a class="page-link" href="/?page=<%= i %>"><%= i %></a></li>
        <% } %>
      <% } %>
    </ul>
  </nav>

</div>