<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title><%= title %></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
  </head>
  <body>
    <%- include('./navFor') %>
     
    <div class="container text-center">
        <Br>
        <a class="btn btn-primary" href="/dasbord/addpost">Addpost</a>  
        <br><br>
        <h2>List Post</h2>
        <div class="list-posts" style="display:  flex; flex-wrap: wrap; justify-content: center;">
          <br>
        <%  getPosts.forEach(e => { %>
          <div class="card" style="width: 30rem; margin-left: 15px; margin-top: 10px;">
            <img src="<%= e.Avatar %>" class="card-img-top" style="height: 300px;"  alt="Foto">
            <div class="card-body">
              <h5 class="card-title"><%= e.Title %></h5>
              <p class="card-text"><%= e.Preparagraf %></p>
              <p class="card-text"><%= e.DatePosts %></p>
              <a href="#" class="btn btn-primary">Go somewhere</a>
            </div>
          </div>
        <% }) %>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>
  </body>
</html>