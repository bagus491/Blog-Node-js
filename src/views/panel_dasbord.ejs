<% if (Role) { %>
    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1>list Users</h1>
    </div>

    <div class="posts-box d-flex">
        <a href="/dasbord">&laquo; back to dasbord</a>
        <div class="box-form" style="margin-left: 15px; margin-bottom: 10px;">
          <form class="d-flex" role="search" action="/dasbord/admin">
            <input class="form-control me-2" type="search" placeholder="Search User" aria-label="Search" name="search_query">
            <button class="btn btn-outline-success" type="submit">Search</button>
          </form>
        </div>
      </div>
      
      <div class="row">
        <div class="col-md-8">
            <table class="table table-success">
                <thead>
                  <tr>
                    <th scope="col">No</th>
                    <th scope="col">User</th>
                    <th scope="col">Role</th>
                  </tr>
                </thead>
                <tbody>
                    <% Users.forEach((User,i) => { i++ %>
                     <tr>
                        <td><%= i %></td>
                        <td><%= User.username %></td>
                        <td><%= User.Role %></td>
                     </tr>
                    <% }) %>
                </tbody>
              </table>
        </div>
      </div>
<% } %>